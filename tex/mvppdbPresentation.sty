\begin{aligned}
& \underset{#1}{\textrm{maximize}}
& & #2 \\
& \textrm{subject to}
& & \begin{aligned} &  #3, \\ &  #4 \end{aligned}
\end{aligned}
}

\newcommand{\maximizethree}[5]{
\begin{aligned}
& \underset{#1}{\textrm{maximize}}
& & #2 \\
& \textrm{subject to}
& & \begin{aligned} &  #3, \\ &  #4 \\ &  #5 \end{aligned}
\end{aligned}
}


